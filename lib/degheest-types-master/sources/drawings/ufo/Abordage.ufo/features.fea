# Prefix: Languagesystems
# automatic
languagesystem DFLT dflt;

feature liga {
sub f f i by f_f_i;
sub f f l by f_f_l;
sub f f by f_f;
sub f i by f_i;
sub f l by f_l;
sub f j by f_j;
sub f t by f_t;
} liga;

feature dlig {
# automatic
lookupflag IgnoreMarks;
sub f j by f_j;
sub f t by f_t;
} dlig;

